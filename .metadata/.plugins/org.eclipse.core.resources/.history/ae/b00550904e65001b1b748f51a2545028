package br.com.academia;

public class Aluno {

	public static void main(String[] args) {
		private String nome;
		private double altura;
		private double peso;
		private int idade;
		
	
		public void setNome(String nome) {
			this.nome = nome;
		}
		
		public String getNome() {
			return this nome;
		}

	}

}
